<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <stroke android:width="2dp" android:color="@color/n2_white_20"/>
            <corners android:radius="4dp"/>
        </shape>
    </item>
    <item>
        <ripple android:color="@color/n2_white_60">
            <item android:id="@android:id/mask">
                <shape android:shape="rectangle">
                    <solid android:color="@color/n2_white"/>
                    <corners android:radius="4dp"/>
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <stroke android:width="2dp" android:color="@color/n2_white_60"/>
                    <corners android:radius="4dp"/>
                </shape>
            </item>
        </ripple>
    </item>
</selector>
