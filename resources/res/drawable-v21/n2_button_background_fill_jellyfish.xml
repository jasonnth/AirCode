<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/n2_fixed_action_footer_transparent_disabled"/>
            <corners android:radius="4dp"/>
        </shape>
    </item>
    <item>
        <ripple android:color="#ffffffff">
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="@color/n2_fixed_action_footer_transparent"/>
                    <corners android:radius="4dp"/>
                </shape>
            </item>
        </ripple>
    </item>
</selector>
