        <com.airbnb.p027n2.primitives.AirButton android:layout_gravity="right|center_vertical|center_horizontal|center" android:id="@+id/account_verifications_next_btn" android:visibility="gone" android:layout_marginBottom="@dimen/n2_vertical_padding_medium" android:text="@string/next" style="@style/n2_BookingNavButton"/>
    </LinearLayout>
</FrameLayout>
