<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.airbnb.p027n2.components.jellyfish.JellyfishView android:id="@+id/jellyfish_view" android:layout_width="match_parent" android:layout_height="match_parent" app:n2_palette="1"/>
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
        <com.airbnb.p027n2.primitives.imaging.HaloImageView android:layout_gravity="left" android:id="@+id/account_verification_preview_profile_photo" android:background="?attr/selectableItemBackgroundBorderless" android:layout_width="@dimen/verifications_halo_large_size" android:layout_height="@dimen/verifications_halo_large_size" android:layout_marginLeft="@dimen/n2_vertical_padding_medium" android:layout_marginTop="?attr/actionBarSize" android:layout_marginBottom="@dimen/n2_vertical_padding_medium" app:n2_placeholder="@drawable/profile_photo_placeholder" app:n2_drawBorder="true" app:n2_borderThickness="@dimen/verifications_halo_border_thickness"/>
        <com.airbnb.p027n2.components.KickerMarquee android:id="@+id/account_verifications_start_header" style="@style/n2_KickerMarquee.NoTopPadding"/>
