<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent" style="@style/n2_Sheet">
    <com.airbnb.p027n2.collections.SheetScrollView android:id="@+id/scroll_view" android:layout_marginBottom="0dp" style="@style/n2_SheetScrollView">
        <LinearLayout style="@style/n2_SheetInputFieldContainer">
            <com.airbnb.p027n2.components.SheetMarquee android:id="@+id/sheet_marquee" style="@style/n2_SheetMarquee"/>
            <com.airbnb.p027n2.primitives.AirButton android:layout_gravity="left" android:id="@+id/done_button" style="@style/n2_AirButton.WhiteFill"/>
        </LinearLayout>
    </com.airbnb.p027n2.collections.SheetScrollView>
    <com.airbnb.p027n2.components.AirToolbar android:id="@+id/toolbar" app:n2_scrollWith="@+id/scroll_view" style="@style/n2_AirToolbar.Transparent.LightForeground"/>
</FrameLayout>
