<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:background="@color/white" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.airbnb.p027n2.collections.VerboseScrollView android:id="@+id/scroll_view" android:layout_width="match_parent" android:layout_height="match_parent">
        <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
            <com.airbnb.p027n2.components.DocumentMarquee android:id="@+id/document_marquee" app:n2_captionText="@string/instant_book_tip_host_advantage" app:n2_titleText="@string/manage_listing_booking_item_instant_book" style="@style/n2_DocumentMarquee"/>
            <com.airbnb.p027n2.components.SimpleTextRow app:n2_text="@string/instant_book_tip_guest_requirements" style="@style/n2_SimpleTextRow.Regular"/>
            <com.airbnb.p027n2.components.LinkActionRow android:id="@+id/house_rules_link" app:n2_text="@string/house_rules" style="@style/n2_LinkActionRow"/>
            <com.airbnb.p027n2.components.LinkActionRow android:id="@+id/guest_requirements_link" app:n2_text="@string/guest_requirements_title" style="@style/n2_LinkActionRow"/>
            <Space android:layout_width="match_parent" android:layout_height="@dimen/n2_vertical_padding_large"/>
        </LinearLayout>
    </com.airbnb.p027n2.collections.VerboseScrollView>
    <com.airbnb.p027n2.primitives.AirButton android:layout_gravity="bottom|right|center_vertical|center_horizontal|center" android:id="@+id/try_it_out_button" android:text="@string/try_it_out" style="@style/n2_AirButton.BabuFill"/>
    <com.airbnb.p027n2.components.AirToolbar android:id="@+id/toolbar" app:n2_scrollWith="@+id/scroll_view" app:n2_navigationIcon="2" style="@style/n2_AirToolbar.Transparent.DarkForeground"/>
</FrameLayout>
