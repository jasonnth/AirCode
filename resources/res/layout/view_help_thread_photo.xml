    <FrameLayout android:id="@+id/loading_view" android:background="@drawable/help_thread_photo_loading_background" android:visibility="gone" android:layout_width="match_parent" app:layout_widthPercent="100%" app:layout_aspectRatio="100%">
        <com.airbnb.p027n2.components.LoadingView android:layout_gravity="center" app:n2_color="@color/white" style="@style/n2_LoadingView.Vertical"/>
    </FrameLayout>
</merge>
