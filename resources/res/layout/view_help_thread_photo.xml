<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <com.airbnb.p027n2.primitives.imaging.AirImageView android:id="@+id/image" android:layout_width="match_parent" android:scaleType="centerCrop" android:cropToPadding="true" app:layout_widthPercent="100%" app:layout_aspectRatio="100%" app:n2_placeholder="@color/white"/>
