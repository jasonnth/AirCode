<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.airbnb.p027n2.components.jellyfish.JellyfishView android:id="@+id/jellyfish_view" android:layout_width="match_parent" android:layout_height="match_parent" app:n2_palette="1"/>
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
        <com.airbnb.p027n2.components.SheetMarquee android:id="@+id/account_verification_finish_marquee" android:paddingBottom="0dp" style="@style/n2_SheetMarquee"/>
        <android.support.p000v4.widget.Space android:layout_width="0dp" android:layout_height="0dp" android:layout_weight="1"/>
        <com.airbnb.p027n2.primitives.imaging.AirImageView android:layout_gravity="center_horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content" app:srcCompat="@drawable/account_verification_selfie_id_compare"/>
        <android.support.p000v4.widget.Space android:layout_width="0dp" android:layout_height="0dp" android:layout_weight="1"/>
        <com.airbnb.p027n2.primitives.AirButton android:layout_gravity="bottom|right|center_vertical|center_horizontal|center" android:id="@+id/account_verification_finish_button" android:text="@string/account_verifications_finish_booking" style="@style/n2_AirButton.WhiteFill.RightCaret.WithText"/>
    </LinearLayout>
</FrameLayout>
