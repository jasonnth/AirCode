<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <FrameLayout android:id="@+id/left_button_container" android:background="@drawable/n2_navigation_pill_selector_left" android:layout_width="wrap_content" android:layout_height="@dimen/n2_navigation_pill_height">
        <LinearLayout android:layout_gravity="center" android:orientation="horizontal" android:paddingRight="@dimen/n2_navigation_pill_padding_horizontal" android:layout_width="wrap_content" android:layout_height="match_parent">
            <com.airbnb.p027n2.primitives.AirTextView android:textSize="@dimen/n2_navigation_pill_text_size" android:textColor="@android:color/black" android:ellipsize="end" android:gravity="center" android:id="@+id/left_button" android:paddingLeft="@dimen/n2_navigation_pill_padding_horizontal" android:paddingTop="@dimen/n2_navigation_pill_padding_vertical" android:paddingRight="@dimen/n2_navigation_pill_icon_padding" android:paddingBottom="@dimen/n2_navigation_pill_padding_vertical" android:layout_width="wrap_content" android:layout_height="match_parent" android:maxWidth="84dp" android:lines="1" app:n2_font="3"/>
            <com.airbnb.p027n2.primitives.imaging.AirImageView android:layout_gravity="left|center_vertical|center_horizontal|center" android:id="@+id/left_button_icon" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
            <com.airbnb.p027n2.primitives.AirTextView android:textSize="@dimen/n2_navigation_pill_text_size" android:textColor="@android:color/white" android:gravity="center" android:id="@+id/left_button_badge" android:background="@drawable/n2_navigation_pill_count_babu" android:visibility="gone" android:layout_width="20dp" android:layout_height="20dp"/>
        </LinearLayout>
    </FrameLayout>
    <View android:id="@+id/divider" android:background="@color/n2_foggy_light" android:layout_width="1dp" android:layout_height="@dimen/n2_navigation_pill_divider_height"/>
    <FrameLayout android:id="@+id/middle_button_container" android:background="@drawable/n2_navigation_pill_selector_middle" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="@dimen/n2_navigation_pill_height">
        <LinearLayout android:layout_gravity="center" android:orientation="horizontal" android:paddingRight="@dimen/n2_navigation_pill_padding_horizontal" android:layout_width="wrap_content" android:layout_height="match_parent">
            <com.airbnb.p027n2.primitives.AirTextView android:textSize="@dimen/n2_navigation_pill_text_size" android:textColor="@android:color/black" android:ellipsize="end" android:gravity="center" android:id="@+id/middle_button" android:paddingLeft="@dimen/n2_navigation_pill_padding_horizontal" android:paddingTop="@dimen/n2_navigation_pill_padding_vertical" android:paddingRight="@dimen/n2_navigation_pill_icon_padding" android:paddingBottom="@dimen/n2_navigation_pill_padding_vertical" android:layout_width="wrap_content" android:layout_height="match_parent" android:maxWidth="84dp" android:lines="1" app:n2_font="3"/>
            <com.airbnb.p027n2.primitives.imaging.AirImageView android:layout_gravity="left|center_vertical|center_horizontal|center" android:id="@+id/middle_button_icon" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
            <com.airbnb.p027n2.primitives.AirTextView android:textSize="@dimen/n2_navigation_pill_text_size" android:textColor="@android:color/white" android:gravity="center" android:id="@+id/middle_button_badge" android:background="@drawable/n2_navigation_pill_count_babu" android:visibility="gone" android:layout_width="20dp" android:layout_height="20dp"/>
        </LinearLayout>
    </FrameLayout>
    <View android:id="@+id/middle_button_divider" android:background="@color/n2_foggy_light" android:visibility="gone" android:layout_width="1dp" android:layout_height="@dimen/n2_navigation_pill_divider_height"/>
    <FrameLayout android:id="@+id/right_button_container" android:background="@drawable/n2_navigation_pill_selector_right" android:layout_width="wrap_content" android:layout_height="@dimen/n2_navigation_pill_height">
        <LinearLayout android:layout_gravity="center" android:orientation="horizontal" android:paddingRight="@dimen/n2_navigation_pill_padding_horizontal" android:layout_width="wrap_content" android:layout_height="match_parent">
            <com.airbnb.p027n2.primitives.AirTextView android:textSize="@dimen/n2_navigation_pill_text_size" android:textColor="@android:color/black" android:ellipsize="end" android:gravity="center" android:id="@+id/right_button" android:paddingLeft="@dimen/n2_navigation_pill_padding_horizontal" android:paddingTop="@dimen/n2_navigation_pill_padding_vertical" android:paddingRight="@dimen/n2_navigation_pill_icon_padding" android:paddingBottom="@dimen/n2_navigation_pill_padding_vertical" android:layout_width="wrap_content" android:layout_height="match_parent" android:maxWidth="84dp" android:lines="1" app:n2_font="3"/>
            <com.airbnb.p027n2.primitives.imaging.AirImageView android:layout_gravity="left|center_vertical|center_horizontal|center" android:id="@+id/right_button_icon" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
            <com.airbnb.p027n2.primitives.AirTextView android:textSize="@dimen/n2_navigation_pill_text_size" android:textColor="@android:color/white" android:gravity="center" android:id="@+id/right_button_badge" android:background="@drawable/n2_navigation_pill_count_babu" android:visibility="gone" android:layout_width="20dp" android:layout_height="20dp"/>
        </LinearLayout>
    </FrameLayout>
</merge>
