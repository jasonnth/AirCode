            <Space android:layout_width="match_parent" android:layout_height="0dp" android:minHeight="@dimen/n2_vertical_padding_small" android:layout_weight="1"/>
            <com.airbnb.p027n2.primitives.AirTextView android:textColorLink="@color/white" android:id="@+id/txt_agreement" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="24dp" style="@style/n2_SmallText.Inverse"/>
        </LinearLayout>
    </ScrollView>
</FrameLayout>
