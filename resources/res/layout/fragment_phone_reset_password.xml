<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent" style="@style/n2_Sheet">
    <com.airbnb.p027n2.components.jellyfish.JellyfishView android:id="@+id/jellyfish_view" android:layout_width="match_parent" android:layout_height="match_parent" app:n2_palette="1"/>
    <com.airbnb.p027n2.collections.SheetScrollView style="@style/n2_SheetScrollView">
        <LinearLayout style="@style/n2_SheetInputFieldContainer">
            <com.airbnb.p027n2.components.SheetMarquee android:id="@+id/sheet_marquee" app:n2_subtitleText="@string/signin_reset_password_phone_desc" app:n2_titleText="@string/signin_reset_password_phone_header" style="@style/n2_SheetMarquee"/>
            <com.airbnb.android.registration.views.PasswordSheetInputText android:id="@+id/phone_number_input_password" android:imeOptions="actionNext" app:n2_hintText="@string/password_stars" style="@style/n2_SheetInputText.Password">
                <requestFocus/>
            </com.airbnb.android.registration.views.PasswordSheetInputText>
        </LinearLayout>
    </com.airbnb.p027n2.collections.SheetScrollView>
    <com.airbnb.p027n2.primitives.AirButton android:enabled="false" android:id="@+id/btn_next" style="@style/n2_AirButton.WhiteFill.RightCaret"/>
</FrameLayout>
