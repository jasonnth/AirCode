<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:background="@color/n2_white" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical" android:clipChildren="false" android:clipToPadding="false" android:layout_width="match_parent" android:layout_height="match_parent">
        <com.airbnb.p027n2.components.AirToolbar android:id="@+id/toolbar" app:n2_navigationIcon="1" style="@style/n2_AirToolbar"/>
        <android.support.p002v7.widget.RecyclerView android:id="@+id/recycler_view" style="@style/n2_PhotoRearrangerRecyclerView"/>
    </LinearLayout>
    <com.airbnb.p027n2.primitives.AirButton android:id="@+id/preview" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/preview" style="@style/n2_AirButton.WhiteFill.BabuBorder.LeftBottom"/>
    <com.airbnb.p027n2.primitives.AirButton android:id="@+id/button_save_rearrange" android:visibility="gone" android:text="@string/manage_listing_photo_button_save_order" style="@style/n2_AirButton.BabuFill.RightBottom"/>
    <com.airbnb.p027n2.primitives.AirButton android:id="@+id/next_btn" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/lys_dls_next" style="@style/n2_AirButton.BabuFill.RightCaret.RightBottom"/>
