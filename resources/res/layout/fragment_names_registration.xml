<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent" style="@style/n2_Sheet">
    <com.airbnb.p027n2.components.jellyfish.JellyfishView android:id="@+id/jellyfish_view" android:layout_width="match_parent" android:layout_height="match_parent" app:n2_palette="1"/>
    <com.airbnb.p027n2.collections.SheetScrollView style="@style/n2_SheetScrollView">
        <LinearLayout style="@style/n2_SheetInputFieldContainer">
            <com.airbnb.p027n2.components.SheetMarquee android:id="@+id/registration_name_sheet_header" app:n2_titleText="@string/registration_name_header" style="@style/n2_SheetMarquee"/>
            <com.airbnb.p027n2.components.SheetInputText android:id="@+id/registration_edit_first_name" android:inputType="textCapWords" android:imeOptions="actionNext" app:n2_hintText="@string/first_name" style="@style/n2_SheetInputText">
                <requestFocus/>
            </com.airbnb.p027n2.components.SheetInputText>
            <com.airbnb.p027n2.components.SheetInputText android:id="@+id/registration_edit_last_name" android:inputType="textCapWords" android:imeOptions="actionNext" app:n2_hintText="@string/last_name" style="@style/n2_SheetInputText"/>
        </LinearLayout>
    </com.airbnb.p027n2.collections.SheetScrollView>
    <com.airbnb.p027n2.primitives.AirButton android:enabled="false" android:id="@+id/registration_edit_names_btn_next" style="@style/n2_AirButton.WhiteFill.RightCaret"/>
</FrameLayout>
