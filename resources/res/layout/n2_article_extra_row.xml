<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <com.airbnb.p027n2.primitives.imaging.AirImageView android:layout_width="wrap_content" android:layout_height="match_parent" app:srcCompat="@drawable/n2_icon_thumb_up"/>
    <com.airbnb.p027n2.primitives.AirTextView android:id="@+id/like_count" android:paddingLeft="@dimen/n2_horizontal_padding_tiny" android:layout_width="wrap_content" android:layout_height="wrap_content" style="@style/n2_SmallText"/>
    <com.airbnb.p027n2.primitives.imaging.AirImageView android:id="@+id/bullet_separator" android:layout_width="4dp" android:layout_height="match_parent" android:layout_marginLeft="@dimen/n2_horizontal_padding_tiny" android:scaleType="centerInside" android:tint="@color/n2_text_color_main" app:srcCompat="@drawable/n2_ic_dot"/>
    <com.airbnb.p027n2.primitives.imaging.AirImageView android:paddingLeft="@dimen/n2_horizontal_padding_tiny" android:layout_width="wrap_content" android:layout_height="match_parent" app:srcCompat="@drawable/n2_icon_comment"/>
    <com.airbnb.p027n2.primitives.AirTextView android:id="@+id/comment_count" android:paddingLeft="@dimen/n2_horizontal_padding_tiny" android:layout_width="wrap_content" android:layout_height="wrap_content" style="@style/n2_SmallText"/>
</merge>
