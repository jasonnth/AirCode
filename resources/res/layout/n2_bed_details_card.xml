    <com.airbnb.p027n2.primitives.AirTextView android:id="@+id/title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxWidth="@dimen/n2_bed_details_card_text_max_width" style="@style/n2_RegularText.Plus"/>
