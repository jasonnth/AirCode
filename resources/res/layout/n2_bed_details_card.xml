<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:background="@drawable/n2_outlined_card_background_drawable" android:padding="@dimen/n2_bed_details_card_padding" android:layout_width="wrap_content" android:layout_height="match_parent" android:minWidth="@dimen/n2_bed_details_card_min_width">
