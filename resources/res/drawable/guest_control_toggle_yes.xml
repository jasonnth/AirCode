    <item android:state_selected="true">
        <shape android:shape="oval">
            <stroke android:width="@dimen/guest_controls_toggle_stroke_width" android:color="@color/babu_button_border"/>
            <solid android:color="@color/c_babu"/>
        </shape>
    </item>
</selector>
